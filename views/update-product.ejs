<!DOCTYPE html>
<html>
<head>
  <title>Update Product</title>
  <link rel="stylesheet" type="text/css" href="/stylesheets/style.css">
</head>
<body>
  <!-- products.ejs -->
<% products.forEach((product) => { %>
  <div class="product">
    <h2><%= product.name %></h2>
    <p><%= product.description %></p>
    <a href="/update-product/<%= product.id %>">Edit</a>
  </div>
<% }) %>

</body>
</html>

</body>
</html>


<!DOCTYPE html>
<html>
<head>
  <title>Product Movements</title>
  <link rel="stylesheet" href="/styles.css">

</head>
<body>
  <h1>Product Movements</h1>
  <table>
    <thead>
      <tr>
        <th>ID</th>
        <th>User ID</th>
        <th>Username</th>
        <th>Product ID</th>
        <th>Product Quantity</th>
        <th>Date</th>
      </tr>
    </thead>
    <tbody>
      <% movements.forEach((movement) => { %>
        <tr>
          <td><%= movement.id %></td>
          <td><%= movement.user_id %></td>
          <td><%= movement.username %></td>
          <td><%= movement.product_id %></td>
          <td><%= movement.product_quantity %></td>
          <td><%= movement.date %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
  <a href="/">Back to Dashboard</a>
</body>
</html>